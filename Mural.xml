<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#DFF3E7"
    tools:context=".Tela_Mural">

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="239dp"
        android:layout_height="45dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="21dp"
        android:layout_marginEnd="81dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toTopOf="@+id/imageView10"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/button8"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/titulo_green_solves" />

    <Button
        android:id="@+id/button7"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="26dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/icon_perfil"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/icon_menu"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <Button
        android:id="@+id/button9"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginEnd="54dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/icon_adicionar"
        app:layout_constraintEnd_toStartOf="@+id/button10"
        tools:layout_editor_absoluteY="660dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/icon_pesquisar"
        tools:layout_editor_absoluteX="227dp"
        tools:layout_editor_absoluteY="660dp" />

    <Button
        android:id="@+id/button11"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="34dp"
        android:layout_marginTop="594dp"
        android:layout_marginEnd="49dp"
        android:background="#00FFFFFF"
        android:drawableBottom="@drawable/icon_mural"
        app:layout_constraintEnd_toStartOf="@+id/button9"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="269dp"
        android:layout_height="178dp"
        android:layout_marginStart="80dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="63dp"
        android:layout_marginBottom="403dp"
        app:layout_constraintBottom_toTopOf="@+id/button7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7"
        app:srcCompat="@drawable/starbucks_1" />

</androidx.constraintlayout.widget.ConstraintLayout>
